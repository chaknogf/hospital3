{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@enums/*": ["app/enum/*"],
      "@shared/*": ["app/shared/*"],
      "@services/*": ["app/services/*"],
      "@models/*": ["app/models/*"]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
